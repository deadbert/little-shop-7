<h1>All Bulk Discounts</h1>
<% @discounts.each do |discount| %>
  <section class="discount_id_<%= discount.id %>">
  <ul>
    <h3><li>Discount id: <%= discount.id %></li></h3>
    <li>Threshold: <%= discount.threshold %></li>
    <li>Discount: <%= discount.discount %>%</li>
  </ul>
  </section>
<% end %>